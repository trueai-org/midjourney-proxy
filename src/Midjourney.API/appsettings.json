{
    "CONCURRENT": null,
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Default": "Warning",
                "System": "Warning",
                "Microsoft": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "logs/log.txt",
                    "rollingInterval": "Day",
                    "fileSizeLimitBytes": null,
                    "rollOnFileSizeLimit": false,
                    "retainedFileCountLimit": 31
                }
            },
            {
                "Name": "Console"
            }
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "urls": "http://*:8080"
}